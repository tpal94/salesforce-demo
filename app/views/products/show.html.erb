<%= render "navs/navbar" %>

<div class="clearfix"></div>
<div id="accounts-container">
  <div class="container" style="padding-top: 125px;">
    <h1>Products</h1>
    <table class="table table-inverse">
      <thead>
        <tr>
          <th>Name</th>
          <th>Product Code</th>
          <th>StockKeepingUnit</th>
        </tr>
      </thead>
      <tbody>
        <% @products.each do |product| %>
          <tr>
            <td><%= product.Name %></td>
            <td><%= product.product_code %></td>
            <td><%= product.stock_keeping_unit %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
